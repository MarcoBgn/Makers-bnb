<section class="container" id='spaces'>
  <ul class="row">
    <% @users_spaces.each do |space| %>
      <li class="list col-lg-12">
        <a id="space" href="/edit/<%=space.id%>"><section id="space">
          <p id='title'><%=space.name%> - <span id='price'>£<%= space.price %> per night</span></p></br>
          <p id='desc'><%=space.description%></p>
          <p>date from:<%=space.available_from%></p>
          <p>date to:<%=space.available_to%></p>
        </section></a>
      </li>
    <% end %>
  </ul>
</section>

<section class="container" id='spaces'>
